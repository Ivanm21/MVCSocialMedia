@model MVCSocialMedia.Models.LogInModel
@{
    ViewBag.Title = "Log In";
}

<h2>Log In</h2>

@Html.ValidationSummary(true)

@using (Html.BeginForm())
{
    @Html.EditorForModel()
    <p>
        <button type="submit">Log In</button>
        <a href="@Url.Action("Register", "Auth")">Register</a>
    </p>
}

